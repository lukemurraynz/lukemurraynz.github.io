{
    "frontMatter.taxonomy.contentTypes": [
        {
            "name": "default",
            "pageBundle": false,
            "fields": [
                {
                    "title": "Title",
                    "name": "title",
                    "type": "string"
                },
                {
                    "title": "Description",
                    "name": "description",
                    "type": "string"
                },
                {
                    "title": "Publishing date",
                    "name": "date",
                    "type": "datetime",
                    "default": "{{now}}",
                    "isPublishDate": true
                },
                {
                    "title": "Content preview",
                    "name": "preview",
                    "type": "image"
                },
                {
                    "title": "Is in draft",
                    "name": "draft",
                    "type": "boolean"
                },
                {
                    "title": "Tags",
                    "name": "tags",
                    "type": "tags"
                },
                {
                    "title": "Categories",
                    "name": "categories",
                    "type": "categories"
                },
                {
                    "title": "metaDescription",
                    "name": "metaDescription",
                    "type": "string"
                },
                {
                    "title": "authors",
                    "name": "authors",
                    "type": "choice",
                    "choices": [
                        "Luke"
                    ]
                },
                {
                    "title": "keywords",
                    "name": "keywords",
                    "type": "choice",
                    "choices": [
                        "mission-critical",
                        "Azure",
                        "Proactive Resiliency",
                        "Assessment",
                        "Cloud Reliability",
                        "Best Practices",
                        "Cloud Reliability",
                        "Best Practices"
                    ]
                }
            ]
        }
    ]
}